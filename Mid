= num_classes\n","\n","        self.learning_rate = 0.1\n","        self.W_HI1 = np.random.randn(inputLayerNeurons, hiddenLayer1Neurons)\n","        self.W_H2H1 = np.random.randn(hiddenLayer1Neurons, hiddenLayer2Neurons)\n","        self.W_OH2 = np.random.randn(hiddenLayer2Neurons, outLayerNeurons)\n","\n","    def sigmoid(self, x, der=False):\n","        if der == True:\n","            return x * (1 - x)\n","        else:\n","            return 1 / (1 + np.exp(-x))\n","\n","    def feedForward(self, X):\n","\n","        #hidden layer1\n","        hidden_input1 = np.dot(X, self.W_HI1)\n","        self.hidden_output1 = self.sigmoid(hidden_input1)\n","\n","        #hidden layer1 to 2\n","        hidden_input2 = np.dot(self.hidden_output1, self.W_H2H1)\n","        self.hidden_output2 = self.sigmoid(hidden_input2)\n","\n","        #hidden layer 2 to Output\n","        output_input = np.dot(self.hidden_output2, self.W_OH2)\n","        pred = self.sigmoid(output_input)\n","\n","        return pred\n","\n","    def backPropagation(self, X, Y, pred):\n","        # output Error\n","        output_error = Y - pred\n","        output_delta = self.learning_rate * output_error * self.sigmoid(pred, der=True)\n","\n","        #hidden layer2 error\n","        hidden_error2 = output_delta.dot(self.W_OH2.T)\n","        hidden_delta2 = self.learning_rate * hidden_error2 * self.sigmoid(self.hidden_output2, der=True)\n","\n","        #hidden layer1\n","        hidden_error1 = hidden_delta2.dot(self.W_H2H1.T)\n","        hidden_delta1 = self.learning_rate * hidden_error1 * self.sigmoid(self.hidden_output1, der=True)\n","\n","        self.W_HI1 += X.T.dot(hidden_delta1)\n","        self.W_H2H1 += self.hidden_output1.T.dot(hidden_delta2)\n","        self.W_OH2 += self.hidden_output2.T.dot(output_delta)\n","\n","    def train(self, X, Y):\n","        output = self.feedForward(X)\n","        self.backPropagation(X, Y, output)"],"metadata":{"id":"qIgyDwiNPx-B","executionInfo":{"status":"ok","timestamp":1763022170629,"user_tz":-360,"elapsed":2,"user":{"displayName":"Emdadul haque Talha","userId":"16563153018440740275"}}},"execution_count":20,"outputs":[]},{"cell_type":"code","source":["NN = NeuralNetwork()\n","epochs = 1000\n","err = []\n","for i in range(epochs):\n","    NN.train(X,Y)\n","    err.append(np.mean(np.square(Y - NN.feedForward(X))))\n","\n","plt.plot(err)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":447},"id":"2YG-qTGbP7Tx","executionInfo":{"status":"ok","timestamp":1763022171204,"user_tz":-360,"elapsed":573,"user":{"displayName":"Emdadul haque Talha","userId":"16563153018440740275"}},"outputId":"7f64f53f-5f76-4947-c29c-7cc50f382601"},"execution_count":21,"outputs":[{"output_type":"execute_result","data":{"text/plain":["[<matplotlib.lines.Line2D at 0x7f01cf619100>]"]},"metadata":{},"execution_count":21},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]}]}
